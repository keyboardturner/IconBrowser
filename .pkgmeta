package-as: IconBrowser
enable-nolib-creation: no

license-output: LICENSE

externals:
  Libs/LibStub: https://repos.wowace.com/wow/libstub/trunk
  Libs/LibRPMedia: https://github.com/wow-rp-addons/LibRPMedia

ignore:
  - Libs/*/*.rockspec
  - Libs/*/*.toc
  - Libs/*/changelog.*
  - Libs/*/CHANGELOG.*
  - Libs/*/docs
  - Libs/*/examples
  - Libs/*/README.*
  - Libs/*/tests
  - Libs/*/.pkgmeta
  - CHANGELOG.md
  - README.md


manual-changelog:
  filename: CHANGELOG.md
  markup-type: markdown